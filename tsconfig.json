{
  "compilerOptions": {
    "baseUrl": "src/",
    "paths": {
      "@/constants/*": ["constants/*"],
      "@/contexts/*": ["contexts/*"],
      "@/data/*": ["data/*"],
      "@/styles/*": ["styles/*"],
      "@/types/*": ["types/*"],
      "@/ui/*": ["ui/*"],
      "@/utils/*": ["utils/*"],
      "@/viewModels/*": ["viewModels/*"]
    },
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", ".next/types/**/*.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
