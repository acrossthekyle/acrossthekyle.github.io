{
  "compilerOptions": {
    "baseUrl": "src/",
    "paths": {
      "@/components": ["components/"],
      "@/components/*": ["components/*"],
      "@/constants": ["constants/"],
      "@/constants/*": ["constants/*"],
      "@/contexts": ["contexts/"],
      "@/contexts/*": ["contexts/*"],
      "@/data": ["data/"],
      "@/data/*": ["data/*"],
      "@/images": ["images/"],
      "@/images/*": ["images/*"],
      "@/styles": ["styles/"],
      "@/styles/*": ["styles/*"],
      "@/types": ["types/"],
      "@/types/*": ["types/*"],
      "@/utils": ["utils/"],
      "@/utils/*": ["utils/*"],
      "@/viewModels": ["viewModels/"],
      "@/viewModels/*": ["viewModels/*"]
    },
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", ".next/types/**/*.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
