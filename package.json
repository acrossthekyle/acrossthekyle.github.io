    "cache-posts": "npx tsx scripts/cache-posts.ts && git add ./src/cache/posts.js"
  },
  "dependencies": {
    "@formspree/react": "3.0.0",
    "@mdx-js/loader": "3.1.0",
    "@mdx-js/react": "3.1.0",
