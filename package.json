{
  "name": "acrossthekyle",
  "version": "1.1.0",
  "private": true,
  "homepage": "https://acrossthekyle.com",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check .",
    "format:fix": "prettier --write . && git add --all",
