    "@stripe/react-stripe-js": "3.6.0",
    "@stripe/stripe-js": "7.1.0",
    "@tmcw/togeojson": "^7.1.0",
    "@xmldom/xmldom": "^0.9.8",
    "chart.js": "4.4.9",
    "csv-parse": "5.6.0",
    "fuse.js": "^7.1.0",
    "gray-matter": "^4.0.3",
    "leaflet": "1.9.4",
    "lodash": "4.17.21",
    "next": "15.2.4",
    "next-mdx-remote": "5.0.0",
    "react": "19.0.0",
    "react-chartjs-2": "5.3.0",
    "react-dom": "19.0.0",
    "react-intersection-observer": "9.16.0",
    "react-leaflet": "^5.0.0",
    "stripe": "18.0.0",
    "ts-node": "10.9.2",
    "uuid": "11.1.0",
    "zustand": "5.0.3"
